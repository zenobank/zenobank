        "operationId": "CurrenciesController_getSupportedTokens_v1",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/TokenResponseDto"
                  }
                }
              }
            }
          }
        },
        "tags": [
