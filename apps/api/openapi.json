    },
    "/api/v1/networks": {
      "get": {
        "description": "Get all networks",
        "operationId": "NetworksController_getNetworks_v1",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/NetworkResponseDto"
                  }
                }
              }
