{
  "name": "api",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "build": "pnpm run db:generate && nest build && pnpm sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "tunnel": "lt -s dkfjadskfjaercl --port 3001",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect",
    "test:e2e": "vitest run --dir test/e2e",
    "db:generate": "pnpm prisma generate",
    "db:pull": "pnpm prisma db pull",
    "db:push": "pnpm prisma db push",
    "db:studio": "pnpm prisma studio",
    "db:fix": "rm -rf node_modules/.prisma/client && pnpm db:generate",
    "db:seed": "pnpm ts-node -r tsconfig-paths/register prisma/seed.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org zeno-yb --project prod-api ./dist && sentry-cli sourcemaps upload --org zeno-yb --project prod-api ./dist"
  },
  "dependencies": {
    "@binance/pay": "^4.0.0",
    "@clerk/backend": "^2.15.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.16.1",
    "@sentry/cli": "^2.56.0",
    "@sentry/nestjs": "^10.18.0",
    "@sentry/profiling-node": "^10.18.0",
    "@types/uuid": "^10.0.0",
    "alchemy-sdk": "^3.6.3",
    "axios": "^1.11.0",
    "bignumber.js": "^9.3.1",
    "bullmq": "^5.56.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.0.2",
    "cookie-parser": "^1.4.7",
    "crypto-convert": "^2.1.7",
    "dotenv": "^17.2.2",
    "easy-currencies": "^1.8.3",
    "express": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "svix": "^1.76.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "viem": "^2.33.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@repo/typescript-config": "workspace:*",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-glob": "^3.3.3",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^6.16.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vitest": "^3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "coverageDirectory": "../coverage"
  }
}
