{
  "name": "@repo/networks",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "^9.31.0",
    "typescript": "5.8.2",
    "@types/node": "^22.15.3",
    "rimraf": "^6.0.1"
  }
}
