{
  "name": "@repo/api-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./model": {
      "import": "./dist/model/index.js",
      "require": "./dist/model/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "": [
        "dist/index.d.ts"
      ],
      "model": [
        "dist/model/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "generate:client": "orval",
    "check-types": "tsc --noEmit",
    "build": "pnpm generate:client && tsc"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@tanstack/react-query": "^5.83.0",
    "@types/node": "^22.15.3",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "eslint": "^9.31.0",
    "orval": "^7.11.2",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.12.2"
  }
}
